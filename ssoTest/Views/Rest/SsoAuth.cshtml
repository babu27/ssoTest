@using System.Linq.Expressions
@model ssoTest.Entity.GradeResponse
           

@{
    ViewBag.Title = "Grade API Testing";
    Layout = null;
}

@Print(()=>Model.AdminUserId)
@Print(()=>Model.Result)

@functions
{

    private static string Print<T>(Expression<Func<T>> expression)
    {
        return string.Format("{0} = {1} ", ((MemberExpression) expression.Body).Member.Name, expression.Compile()());
    }

}
